{
  "name": "redone",
  "version": "0.3.10",
  "description": "Predictable relationships between React components.",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run eslint && npm run build && npm run bundle",
    "build": "rm -rf lib && babel src --out-dir lib",
    "bundle": "rm -rf dist && webpack",
    "test": "jest",
    "eslint": "eslint src/**"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stephenbunch/redone"
  },
  "author": "Stephen Bunch",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "jest": "^15.1.1",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "webpack": "^1.13.2"
  },
  "peerDependencies": {
    "react": "^15.3.2"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "testPathDirs": [
      "<rootDir>/src/"
    ],
    "testRegex": "\\.spec\\.jsx?$"
  }
}
